{
	"name": "instagram-follower-supabase",
	"version": "1.0.0",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"types": "supabase gen types typescript --local > src/lib/database.types.ts 2>/dev/null && prettier --write src/lib/database.types.ts",
		"watch:types": "chokidar 'src/**/*' --ignore 'src/lib/database.types.ts' -c 'yarn types'",
		"dev:all": "concurrently \"yarn dev\" \"yarn watch:types\""
	},
	"devDependencies": {
		"@playwright/test": "^1.28.1",
		"@skeletonlabs/skeleton": "^2.11.0",
		"@skeletonlabs/tw-plugin": "^0.4.1",
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@tailwindcss/forms": "^0.5.10",
		"@tailwindcss/typography": "^0.5.16",
		"@types/eslint": "8.56.0",
		"@types/node": "^22.13.10",
		"@typescript-eslint/eslint-plugin": "^6.0.0",
		"@typescript-eslint/parser": "^6.0.0",
		"autoprefixer": "^10.4.21",
		"chokidar-cli": "^3.0.0",
		"concurrently": "^9.1.2",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.35.1",
		"postcss": "^8.5.3",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.1.2",
		"supabase": "^2.19.7",
		"svelte": "^4.2.7",
		"svelte-check": "^3.6.0",
		"tailwindcss": "^3.4.17",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^5.0.3",
		"vite-plugin-tailwind-purgecss": "^0.3.5"
	},
	"type": "module",
	"dependencies": {
		"@floating-ui/dom": "^1.6.13",
		"@supabase/supabase-js": "^2.49.1",
		"axios": "^1.8.3",
		"highlight.js": "^11.11.1",
		"lucide-svelte": "^0.483.0"
	}
}
